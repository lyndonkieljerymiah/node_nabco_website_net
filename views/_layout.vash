<!DOCTYPE html>
<html lang="en">
    <head>

        <title>@model.title</title>

        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1">

        <link href="/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />
        <link href="/lib/font-awesome/css/font-awesome.min.css" rel="stylesheet" />
        <link href="/dist/css/default.min.css" rel="stylesheet" />
        <link href="/dist/css/social.min.css" rel="stylesheet" />

    </head>
    <body>
        <header class="nb-header">
            <div class="container">
                <div class="row">
                    <div class="col-md-5">
                        <img src="/imgs/nabco.png" class="nb-logo" />
                    </div>
                </div>
            </div>
            <nav class="nb-navbar visible-md visible-lg">
                <div class="container">
                    <ul>
                        <li><a href="#">New Arrivals</a></li>
                        <li><a href="#">Living Room</a></li>
                        <li><a href="#">Dining Room</a></li>
                        <li><a href="#">Bed Room</a></li>
                        <li><a href="#">Hall</a></li>
                        
                    </ul>
                </div>
            </nav>
        </header>
        <div>
            @html.block("body")
        </div>

        <!--drawer -->
        <div class="hidden-xs">
            <div id="drawer" class="drawer stk-show">
                <header class="heading">
                    <p id="drawer-title">Facebook</p>
                    <div class="dark-cover"></div>
                </header>
                
                <div class="content">
                    <div id="fb-drawer-content">
                        <iframe src="https://www.facebook.com/plugins/page.php?href=https%3A%2F%2Fwww.facebook.com%2FNABCO-Furniture-Centre-1503548559966817%2F&tabs=timeline&width=340&height=680&small_header=false&adapt_container_width=true&hide_cover=false&show_facepile=true&appId=1805387546380028"
                                height="560" style="border:none;overflow:hidden;width:100%" scrolling="no" frameborder="0" allowTransparency="true"></iframe>
                    </div>
                    <div id="youtube-drawer-content" style="display:none">
                        <div class="nb-slider">
                            <div class="nb-slider-container">
                                <ul>
                                    <li style="opacity:1"><iframe class="nb-slider-video" src="https://www.youtube.com/embed/j5jIjI_Ccqc" frameborder="0" allowfullscreen></iframe></li>
                                    <li style="opacity:1"><iframe class="nb-slider-video" src="https://www.youtube.com/embed/DTKQONRv6Ng" frameborder="0" allowfullscreen></iframe></li>
                                    <li style="opacity:1"><iframe class="nb-slider-video" src="https://www.youtube.com/embed/Z1eKNuno7Uw" frameborder="0" allowfullscreen></iframe></li>
                                    <li style="opacity:1"><iframe class="nb-slider-video" src="https://www.youtube.com/embed/QufJ0ctfZG0" frameborder="0" allowfullscreen></iframe></li>
                                    <li style="opacity:1"><iframe class="nb-slider-video" src="https://www.youtube.com/embed/eVbP8564ZR0" frameborder="0" allowfullscreen></iframe></li>
                                </ul>
                            </div>
                            <div class="nb-slider-control">
                                <a id="downButton" href="#"><i class="fa fa-arrow-circle-down"></i></a>
                                <a id="upButton" href="#"><i class="fa fa-arrow-circle-up"></i></a>
                            </div>
                        </div>
                    </div>
                    <div id="comment-drawer-content" style="display:none">
                        <form class="form">
                            <div class="form-group">
                                <label>Email Address:</label>
                                <div class="input-group">
                                    <span class="input-group-addon"><i class="fa fa-envelope-open fa-fw"></i></span>
                                    <input type="text" name="emailAddress" class="form-control" placeholder="Email Address">
                                </div>
                            </div>
                            <div class="form-group">
                                <label>Mobile No:</label>
                                <div class="input-group">
                                    <span class="input-group-addon"><i class="fa fa-mobile-phone fa-fw"></i></span>
                                    <input type="text" name="mobileNo" class="form-control" placeholder="Mobile No">
                                </div>
                            </div>
                            <div class="form-group">
                                <label>Subject No:</label>
                                <div class="input-group">
                                    <span class="input-group-addon"><i class="fa fa-align-justify fa-fw"></i></span>
                                    <input type="text" name="subject" class="form-control" placeholder="Subject">
                                </div>
                            </div>
                            <div class="form-group">
                                <label>Message:</label>
                                <div><textarea type="text" name="message" class="form-control"></textarea> </div>
                            </div>
                            <div>
                                <button class="btn btn-info">Send</button>
                            </div>
                        </form>
                    </div>
                    <div id="location-drawer-content" style="display:none">
                        <div class="nb-location">
                            <div class="location-wrapper">
                                <ul>
                                    <li>
                                        <div class="map-content">
                                            <iframe src="https://www.google.com/maps/embed?pb=!1m14!1m8!1m3!1d3609.0619003262086!2d51.573588!3d25.23484!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x3e45cee18e475f97%3A0x3e1da4faed0d794c!2zTmFiY28gRnVybml0dXJlIENlbnRlciDZhdix2YPYsiDZhtin2KjZg9mIINmE2YTZhdmB2LHZiNi02KfYqg!5e0!3m2!1sen!2s!4v1489251350100"
                                                    frameborder="0" style="border:0;width:100%;height:100%" allowfullscreen></iframe>
                                        </div>
                                        <div>
                                            <address style="margin-bottom:0">
                                                <strong classs="text-mute">Wakrah Branch</strong><br>
                                                <small>
                                                    Airport Street, Wakra Road P.O. Box 1654, <br>
                                                    Doha State of Qatar<br>
                                                    <abbr title="Phone"><i class="fa fa-phone"></i></abbr> +(974) 4466-66-46
                                                </small>
                                            </address>
                                        </div>
                                    </li>
                                    <li>
                                        <div class="map-content">
                                            <iframe src="https://www.google.com/maps/embed?pb=!1m14!1m8!1m3!1d28851.647069169227!2d51.46848!3d25.322479!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x0%3A0x677700b4dc79f3ea!2sNABCO+FURNITURE+CENTRE%2C+Garaffah+Branch!5e0!3m2!1sen!2s!4v1489251717884"
                                                    frameborder="0" style="border:0;width:100%;height:100%" allowfullscreen></iframe>
                                        </div>
                                        <div>
                                            <address style="margin-bottom:0">
                                                <strong>Garaffah Branch</strong><br>
                                                <small>
                                                    Beside Immigration Office, Garaffah, <br>
                                                    Doha State of Qatar<br>
                                                    <abbr title="Phone"><i class="fa fa-phone"></i></abbr> +(974) 4466-66-36
                                                </small>
                                            </address>
                                        </div>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </div>
                    
                    
                </div>
                <footer class="footer">
                    <ul>
                        <li>
                            <a id="social-fb" class="icon drawer-icon-bg bg-blue active" data-container="fb-drawer-content" data-title="Facebook">
                                <span><i class="fa fa-facebook"></i></span>
                            </a>
                        </li>
                        <li>
                            <a id="social-youtube" class="icon drawer-icon-bg bg-red" data-container="youtube-drawer-content" data-title="Youtube">
                                <span><i class="fa fa-youtube-play"></i></span>
                            </a>
                        </li>
                        <li>
                            <a id="social-comment" class="icon drawer-icon-bg bg-gray" data-container="comment-drawer-content" data-title="Comment">
                                <span><i class="fa fa-envelope-o"></i></span>
                            </a>
                        </li>
                        <li>
                            <a id="social-location" class="icon drawer-icon-bg bg-creme" data-container="location-drawer-content" data-title="Location">
                                <span><i class="fa fa-map-marker"></i></span>
                            </a>
                        </li>
                        <li>
                            <a id="social-instagram" class="icon drawer-icon-bg bg-creme" data-container="location-drawer-content" data-title="Location">
                                <span><i class="fa fa-instagram"></i></span>
                            </a>
                        </li>
                        <li>
                            <a id="social-twitter" class="icon drawer-icon-bg bg-creme" data-container="location-drawer-content" data-title="Location">
                                <span><i class="fa fa-twitter"></i></span>
                            </a>
                        </li>
                    </ul>

                </footer>
            </div>
        </div>
        <!--end-->

		<!--chat -->
		<div class="chat-holder">
			<div class="chat-icon-open"></div>
		</div>
		<!--end-->

        <script src="/lib/jquery/dist/jquery.min.js"></script>
        <script src="/dist/scripts/toolset.min.js"></script>
        
        <script>
            $(document).ready(function ()
            {   

                var obj2 = {
                    target: $("#drawer"),
                    default: "#fb-drawer-content",
                    pullIn: { top: 0 },
                    pullOut: { top: -50 },
                    headerTarget: "#drawer-title"
                }

                var slider = new Slider();
                var drawer = new Sticker(obj2);

                slider.init();
                drawer.init();
            });
        </script>
    </body>
</html>